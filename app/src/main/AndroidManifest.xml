<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jebora.jebora" >

    <uses-sdk
        android:maxSdkVersion="22"
        android:minSdkVersion="16" />

    <uses-permission android:name="android.permission.CAMERA" />

    <user-feature android:name="android.hardware.camera" />
    <user-feature android:name="android.permission.autofocus" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:theme="@style/CustomActionBarTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:launchMode="singleTop" >
        </activity>
        <activity
            android:name=".SignUp"
            android:label="@string/title_activity_sign_up"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".Login"
            android:label="@string/title_activity_login"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".SignUp_2"
            android:label="@string/title_activity_sign_up_2"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".SignUp" />
        </activity>
        <activity
            android:name=".PhotoGridView"
            android:label="@string/title_activity_photo_grid_view"
            android:parentActivityName=".UserMain" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".UserMain" />
        </activity>
        <activity
            android:name=".ChooseProduct"
            android:label="@string/title_activity_choose_product"
            android:parentActivityName=".UserMain" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".UserMain" />
        </activity>
        <activity
            android:name=".PreviewProduct"
            android:label="@string/title_activity_preview_product"
            android:parentActivityName=".UserMain" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".UserMain" />
        </activity>
        <activity
            android:name=".PayvementInfo"
            android:label="@string/title_activity_payvement_info"
            android:parentActivityName=".PreviewProduct" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".PreviewProduct" />
        </activity>
        <activity
            android:name=".UserMain"
            android:label="@string/title_activity_user_main" >
        </activity>
        <activity
            android:name=".LoadModels"
            android:label="loadModel"
            android:parentActivityName=".PreviewProduct" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".PreviewProduct" />
        </activity>
        <activity
            android:name=".ImageEditing"
            android:exported="true"
            android:label="@string/title_activity_image_editing"
            android:parentActivityName=".DetailActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" >
        </activity>
        <activity
            android:name=".ProductSelect"
            android:label="ProductSelect"
            android:parentActivityName=".UserMain" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".UserMain" />
        </activity>
        <activity
            android:name=".AddKids"
            android:label="@string/title_activity_add_kids"
            android:parentActivityName=".UserMain" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.jebora.jebora.UserMain" />
        </activity>
        <activity
            android:name=".ManageKids"
            android:label="@string/title_activity_manage_kids"
            android:parentActivityName=".UserMain" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.jebora.jebora.UserMain" />
        </activity>
        <activity
            android:name=".EditKid"
            android:label="@string/title_activity_edit_kid"
            android:parentActivityName=".ManageKids" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.jebora.jebora.ManageKids" />
        </activity>
        <activity
            android:name=".DetailActivity"
            android:label="@string/title_activity_details"
            android:parentActivityName=".UserMain" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.jebora.jebora.UserMain" />
        </activity>
        <activity
            android:name=".ShoppingCart"
            android:label="@string/title_activity_shopping_cart"
            android:parentActivityName=".UserMain" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.jebora.jebora.UserMain" />
        </activity>
        <activity
            android:name=".Shipping"
            android:label="@string/title_activity_shipping_info"
            android:parentActivityName=".ShoppingCart" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.jebora.jebora.ShoppingCart" />
        </activity>
        <activity
            android:name=".AccountInfo"
            android:label="@string/title_activity_account_info"
            android:parentActivityName=".UserMain" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.jebora.jebora.UserMain" />
        </activity>
        <activity
            android:name=".Checkout"
            android:label="@string/title_activity_checkout" >
        </activity>
        <activity
            android:name=".ManageAddress"
            android:label="@string/title_activity_manage_address"
            android:parentActivityName=".Shipping" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Shipping" />
        </activity>
        <activity
            android:name=".EditAddress"
            android:label="@string/title_activity_edit_address"
            android:parentActivityName=".ManageAddress" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.jebora.jebora.ManageAddress" />
        </activity>
        <activity
            android:name=".SplashActivity"
            android:label="@string/title_activity_splash"
            android:theme="@style/NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
